{
    "name": "balsama/boston-neighborhood-finder",
    "description": "Determines what Boston neighborhood a lat long pair is in.",
    "type": "project",
    "require": {
        "php": "^8",
        "ext-json": "*",
        "mjaschen/phpgeo": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "license": "gpl-2.0-or-later",
    "autoload": {
        "psr-4": {
            "Balsama\\BostonNeighborhoodFinder\\": "src/",
            "Balsama\\BostonNeighborhoodFinder\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {},
    "config": {
        "platform": {
            "php": "8.0"
        }
    },
    "scripts": {
        "lint": "./vendor/bin/phpcs --standard=PSR12 ./src ./tests",
        "test": "./vendor/bin/phpunit tests"
    }
}
